         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ProposalReview">
      <xsd:annotation>
         <xsd:documentation>A review of a proposal</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposalManagement:ProposalReview</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="comment" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Description</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:ProposalReview.comment</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="score" type="xsd:float" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>the review score</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:ProposalReview.score</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="technicalFeasibility"
                      type="xsd:boolean"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>is the proposal technically feasible</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:ProposalReview.technicalFeasibility</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="reviewDate"
                      type="xsd:dateTime"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>the date that the review is submitted</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:ProposalReview.reviewDate</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <!--this is a reference-->
         <xsd:element name="reviewer" type="xsd:IDREF" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>the reviewer making this review</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:ProposalReview.reviewer</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ResourceType">
      <xsd:annotation>
         <xsd:documentation>a type of resource  </xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposalManagement:ResourceType</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>the name of the resource type</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:ResourceType.name</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="unit" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:ResourceType.unit</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="_id" type="xsd:ID"/>
   </xsd:complexType>
   <xsd:complexType name="Resource">
      <xsd:annotation>
         <xsd:documentation>A resource that will be consumed by allocating an observation from a proposal</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposalManagement:Resource</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="amount" type="xsd:float" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The amount of the resource</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:Resource.amount</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <!--this is a reference-->
         <xsd:element name="type" type="xsd:IDREF" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>the type of the resource</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:Resource.type</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="AvailableResources">
      <xsd:annotation>
         <xsd:documentation>The list of resources that are available</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposalManagement:AvailableResources</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="resources"
                      type="propm:Resource"
                      minOccurs="0"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="AllocationGrade">
      <xsd:annotation>
         <xsd:documentation>The final grade given by the TAC</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposalManagement:AllocationGrade</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="name" type="xsd:string" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>the name of the grade</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:AllocationGrade.name</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="description"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>what this grade means</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:AllocationGrade.description</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="_id" type="xsd:ID"/>
   </xsd:complexType>
   <xsd:complexType name="ResourceBlock">
      <xsd:annotation>
         <xsd:documentation>A block of resources that can be allocated</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposalManagement:ResourceBlock</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="resource"
                      type="propm:Resource"
                      minOccurs="1"
                      maxOccurs="1"/>
         <!--this is a reference-->
         <xsd:element name="mode" type="xsd:IDREF" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:ResourceBlock.mode</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="AllocatedBlock">
      <xsd:annotation>
         <xsd:documentation>A block of resources that have been allocated</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposalManagement:AllocatedBlock</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="propm:ResourceBlock">
            <xsd:sequence><!--this is a reference-->
               <xsd:element name="grade" type="xsd:IDREF" minOccurs="1" maxOccurs="1">
                  <xsd:annotation>
                     <xsd:documentation/>
                     <xsd:appinfo>
                        <vodml-ref>proposalManagement:AllocatedBlock.grade</vodml-ref>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:simpleType name="InstrumentKind">
      <xsd:annotation>
         <xsd:documentation/>
         <xsd:appinfo>
            <vodml-ref>proposalManagement:InstrumentKind</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:string">
         <xsd:enumeration value="continuum">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:InstrumentKind.continuum</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="spectroscopic">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:InstrumentKind.spectroscopic</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="TacRole">
      <xsd:annotation>
         <xsd:documentation>A role within the timeAllocation committee</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposalManagement:TacRole</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:string">
         <xsd:enumeration value="TechnicalReviewer">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:TacRole.TechnicalReviewer</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="ScienceReviewer">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:TacRole.ScienceReviewer</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Chair">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposalManagement:TacRole.Chair</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
</xsd:schema>
