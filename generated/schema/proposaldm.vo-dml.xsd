         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="spectralWindowSetup"
                      type="prop:SpectralWindowSetup"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:ScienceSpectralWindow.spectralWindowSetup</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="expectedSpectralLine"
                      type="prop:ExpectedSpectralLine"
                      minOccurs="0"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="ExpectedSpectralLine">
      <xsd:annotation>
         <xsd:documentation>Brief description of a spectral line.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposal:ExpectedSpectralLine</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="restFrequency"
                      type="ivoa:RealQuantity"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:ExpectedSpectralLine.restFrequency</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="transition" type="xsd:string" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:ExpectedSpectralLine.transition</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="splatalogId"
                      type="ivoa:stringIdentifier"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:ExpectedSpectralLine.splatalogId</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="description"
                      type="xsd:string"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:ExpectedSpectralLine.description</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="PerformanceParameters">
      <xsd:annotation>
         <xsd:documentation>The parameters required for observation to be useful for the science goal</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposal:PerformanceParameters</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="desiredAngularResolution"
                      type="ivoa:RealQuantity"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:PerformanceParameters.desiredAngularResolution</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="desiredLargestScale"
                      type="ivoa:RealQuantity"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:PerformanceParameters.desiredLargestScale</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="desiredSensitivity"
                      type="ivoa:RealQuantity"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:PerformanceParameters.desiredSensitivity</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="desiredDynamicRange"
                      type="ivoa:RealQuantity"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:PerformanceParameters.desiredDynamicRange</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="representativeSpectralPoint"
                      type="ivoa:RealQuantity"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>the the point in the EM where these parameters should be evaluated</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:PerformanceParameters.representativeSpectralPoint</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="Observation" abstract="true">
      <xsd:annotation>
         <xsd:documentation>An observation - a pointing of the telescope at a part of the sky, occurs in a single non-overlapping time period</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposal:Observation</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="constraints"
                      type="prop:ObservingConstraint"
                      minOccurs="0"
                      maxOccurs="unbounded"/>
         <!--this is a reference-->
         <xsd:element name="target"
                      type="xsd:IDREF"
                      minOccurs="0"
                      maxOccurs="unbounded">
            <xsd:annotation>
               <xsd:documentation>The actual target of the observation</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:Observation.target</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <!--this is a reference-->
         <xsd:element name="field" type="xsd:IDREF" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The Field for the observation</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:Observation.field</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <!--this is a reference-->
         <xsd:element name="technicalGoal"
                      type="xsd:IDREF"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The technical goals of the observation</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:Observation.technicalGoal</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="_id" type="xsd:ID"/>
   </xsd:complexType>
   <xsd:complexType name="TargetObservation">
      <xsd:annotation>
         <xsd:documentation>an observation of the scientific target </xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposal:TargetObservation</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="prop:Observation"/>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="CalibrationObservation">
      <xsd:annotation>
         <xsd:documentation>An observation that is intended for calibration</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposal:CalibrationObservation</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:complexContent>
         <xsd:extension base="prop:Observation">
            <xsd:sequence>
               <xsd:element name="intent"
                            type="prop:CalibrationTarget_intendedUse"
                            minOccurs="1"
                            maxOccurs="1">
                  <xsd:annotation>
                     <xsd:documentation>The use of the calibration observation</xsd:documentation>
                     <xsd:appinfo>
                        <vodml-ref>proposal:CalibrationObservation.intent</vodml-ref>
                     </xsd:appinfo>
                  </xsd:annotation>
               </xsd:element>
            </xsd:sequence>
         </xsd:extension>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="TechnicalGoal">
      <xsd:annotation>
         <xsd:documentation>collects together the technical goals of the proposal</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposal:TechnicalGoal</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="performance"
                      type="prop:PerformanceParameters"
                      minOccurs="1"
                      maxOccurs="1"/>
         <xsd:element name="spectrum"
                      type="prop:ScienceSpectralWindow"
                      minOccurs="1"
                      maxOccurs="unbounded"/>
      </xsd:sequence>
      <xsd:attribute name="_id" type="xsd:ID"/>
   </xsd:complexType>
   <xsd:complexType name="SpectralWindowSetup">
      <xsd:annotation>
         <xsd:documentation>Science oriented definition of a spectral window.</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposal:SpectralWindowSetup</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="start"
                      type="ivoa:RealQuantity"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The start of the spectral window</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:SpectralWindowSetup.start</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="end" type="ivoa:RealQuantity" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>The end of the spectral window</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:SpectralWindowSetup.end</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="spectralResolution"
                      type="ivoa:RealQuantity"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>channel width</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:SpectralWindowSetup.spectralResolution</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="isSkyFrequency"
                      type="xsd:boolean"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>represents an observed frequency</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:SpectralWindowSetup.isSkyFrequency</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="polarization"
                      type="coord:PolStateEnum"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>desired polarization</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:SpectralWindowSetup.polarization</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:simpleType name="WikiDataId">
      <xsd:annotation>
         <xsd:documentation>a WikiData identifier</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposal:WikiDataId</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:string"/>
   </xsd:simpleType>
   <xsd:simpleType name="ProposalKind">
      <xsd:annotation>
         <xsd:documentation/>
         <xsd:appinfo>
            <vodml-ref>proposal:ProposalKind</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:string">
         <xsd:enumeration value="Standard">
            <xsd:annotation>
               <xsd:documentation>Normal proposal</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:ProposalKind.Standard</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="ToO">
            <xsd:annotation>
               <xsd:documentation>target of opportunity</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:ProposalKind.ToO</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Survey">
            <xsd:annotation>
               <xsd:documentation>Part of a large programme</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:ProposalKind.Survey</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="InvestigatorKind">
      <xsd:annotation>
         <xsd:documentation/>
         <xsd:appinfo>
            <vodml-ref>proposal:InvestigatorKind</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:string">
         <xsd:enumeration value="PI">
            <xsd:annotation>
               <xsd:documentation>principal investigator</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:InvestigatorKind.PI</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="COI">
            <xsd:annotation>
               <xsd:documentation>co-investigator</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:InvestigatorKind.COI</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="TextFormats">
      <xsd:annotation>
         <xsd:documentation>acceptable text formats for document submission</xsd:documentation>
         <xsd:appinfo>
            <vodml-ref>proposal:TextFormats</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:string">
         <xsd:enumeration value="latex">
            <xsd:annotation>
               <xsd:documentation>LaTeX using standard document style</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:TextFormats.latex</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="rst">
            <xsd:annotation>
               <xsd:documentation>restructured text</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:TextFormats.rst</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="asciidoc">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:TextFormats.asciidoc</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
   <xsd:simpleType name="CalibrationTarget_intendedUse">
      <xsd:annotation>
         <xsd:documentation/>
         <xsd:appinfo>
            <vodml-ref>proposal:CalibrationTarget_intendedUse</vodml-ref>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:string">
         <xsd:enumeration value="Amplitude">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:CalibrationTarget_intendedUse.Amplitude</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Atmospheric">
            <xsd:annotation>
               <xsd:documentation>used to measure atmospheric effects</xsd:documentation>
               <xsd:appinfo>
                  <vodml-ref>proposal:CalibrationTarget_intendedUse.Atmospheric</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Bandpass">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:CalibrationTarget_intendedUse.Bandpass</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Phase">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:CalibrationTarget_intendedUse.Phase</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Pointing">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:CalibrationTarget_intendedUse.Pointing</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Focus">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:CalibrationTarget_intendedUse.Focus</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Polarization">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:CalibrationTarget_intendedUse.Polarization</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
         <xsd:enumeration value="Delay">
            <xsd:annotation>
               <xsd:documentation/>
               <xsd:appinfo>
                  <vodml-ref>proposal:CalibrationTarget_intendedUse.Delay</vodml-ref>
               </xsd:appinfo>
            </xsd:annotation>
         </xsd:enumeration>
      </xsd:restriction>
   </xsd:simpleType>
</xsd:schema>
